  (defcolumn "JB1-1-2-2_low" deBruin 
    (Soil 
      (MaxRootingDepth 60 [cm])
      (border -100 [cm] -150 [cm] -300 [cm])
      (horizons 
        ( -30 [cm] "Ap_JB1_low_OM")
        ( -60 [cm] "B_JB1_")
        ( -80 [cm] "B_JB2_")
        ( -250 [cm] "C_JB2") ) )
    (Groundwater deep)
    (Chemistry multi 
      (combine 
        (N 
          (reaction nitrification 
            (denitrification 
              (water_factor 
                (0.6 0.0)
                (0.8 0.1)
                (0.9 0.2)
                (1.0 1.0)) )))))
