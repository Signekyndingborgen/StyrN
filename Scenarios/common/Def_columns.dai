(defaction "irrigate_30" activity
(wait (and (after_mm_dd 5 1)
(before_mm_dd 9 1)(not (soil_water_pressure_above (height -30.0 [cm])
(potential -600 [cm]))))) ; måske -400 cm
(irrigate_overhead 30 [mm/h])
	(wait_days 5))