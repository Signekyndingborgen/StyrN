    (Soil 
      (MaxRootingDepth 60 [cm])
      (border -100 [cm] -150 [cm] -300 [cm])
      (horizons 
        ( -30 [cm] "Ap_JB1_low_OM")
        ( -60 [cm] "B_JB1_")
